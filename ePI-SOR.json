    "/integration/product/{gtin}": {
      "post": {
        "tags": [
          "Products"
        ],
        "summary": "Create or Update a product",
        "description": "Takes a unique gtin and creates or updates a product with that gtin.",
        "operationId": "addProduct",
        "parameters": [
          {
            "name": "gtin",
            "in": "path",
            "description": "Global Trade Item Number of Product",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "Creates or Update a product.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Operation"
          },
          "400": {
            "description": "Invalid Parameter"
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Not Found"
          },
          "415": {
            "description": "Unsupported Media Type"
          },
          "422": {
            "description": "Unprocessable Entity"
          },
          "500": {
            "description": "Server Error"
          }
        }
      },
      "get": {
        "tags": [
          "DSU Fabric"
        ],
        "summary": "Display metadata for products",
        "description": "Returns the whole product metadata",
        "operationId": "readProductsMetadata",
        "parameters": [
          {
            "name": "gtin",
            "in": "path",
            "description": "Global Trade Item Number of Product",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          },
          "400": {
            "description": "Invalid Parameter"
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Product not found"
          },
          "415": {
            "description": "Unsupported Media Type"
          },
          "422": {
            "description": "Unprocessable Entity"
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    },
